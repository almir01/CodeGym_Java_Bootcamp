public class Solution {

    public static void main(String[] args) {
    
        Cat cat1 = new Cat();
        Cat.count++;   //this line of code was added

        Cat cat2 = new Cat();
        Cat.count++;  //this line of code was added

        System.out.println("The cat count is " + Cat.count);
    }


    public static class Cat {   
        public static int count = 0;
    }
    
    
}
