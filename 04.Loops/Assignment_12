public class Solution {

    public static void main(String[] args) throws Exception {
    
        Scanner sc = new Scanner(System.in);
       
       int sum = 0;

       while (true) {

         int  num = sc.nextInt();


           if (num == -1) {
               sum = sum -1;
               break;
           }
           sum +=num;
       }
        System.out.println(sum);


    }
}
